
#include <bits/stdc++.h>

using namespace std;

void permu(vector<int>&v,int ind,vector<vector<int>>&ans)
{   
    if(ind==v.size())
    {
        for(int i=0;i<v.size();i++)
        {
            cout<<v[i]<<" ";
        }
        cout<<endl;
        
        ans.push_back(v);
        return;
    }
    
    for(int i=ind;i<v.size();i++)
    {   
        
        swap(v[i],v[ind]);
        permu(v,ind+1,ans);
        swap(v[i],v[ind]);
    }
}



int main()
{
    vector<int>v={1,2,3};
    vector<vector<int>>ans;
    permu(v,0,ans);
   
}
